<p id="notice"><%= notice %></p>

<h1>Listing Shortens</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Shortcode</th>
      <th>Startdate</th>
      <th>Lastseendate</th>
      <th>Redirectcount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shortens.each do |shorten| %>
      <tr>
        <td><%= shorten.url %></td>
        <td><%= shorten.shortcode %></td>
        <td><%= shorten.startdate %></td>
        <td><%= shorten.lastseendate %></td>
        <td><%= shorten.redirectcount %></td>
        <td><%= link_to 'Show', shorten %></td>
        <td><%= link_to 'Edit', edit_shorten_path(shorten) %></td>
        <td><%= link_to 'Destroy', shorten, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shorten', new_shorten_path %>
